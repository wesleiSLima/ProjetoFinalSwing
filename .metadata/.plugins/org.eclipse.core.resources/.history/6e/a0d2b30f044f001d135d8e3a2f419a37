package br.com.voeairlines.model;

public class TipoAeronaveDTO {

	// atributo
	private int id;
	private String descricao;

	// get e set
	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

}
